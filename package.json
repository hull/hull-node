{
  "name": "hull",
  "version": "0.13.6",
  "description": "A Node.js client for hull.io",
  "main": "lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/hull/hull-node.git"
  },
  "homepage": "https://github.com/hull/hull-node",
  "bugs": {
    "url": "https://github.com/hull/hull-node/issues"
  },
  "author": "Romain Dardour <romain@hull.io>",
  "license": "MIT",
  "scripts": {
    "test": "npm run test:lint && npm run test:flow && npm run test:unit && npm run test:integration",
    "test:lint": "eslint src",
    "test:modules": "npm outdated --depth=0",
    "test:unit": "NODE_ENV=test mocha --require babel-register -R spec ./test/unit/*.js ./test/unit/**/*.js",
    "test:integration": "NODE_ENV=test mocha --require babel-register -R spec ./test/integration/*.js",
    "test:flow": "flow check",
    "test:coverage": "nyc --babel-cache --hook-run-in-context false npm run test",
    "update": "updtr",
    "clean": "rimraf lib",
    "build": "gulp build",
    "watch": "gulp watch",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "JSONStream": "^1.1.2",
    "aws-sdk": "^2.81.0",
    "basic-auth": "^1.1.0",
    "batch-stream": "^0.1.3",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "bull": "^3.0.0-rc.3",
    "cache-manager": "^2.1.2",
    "connect": "^3.4.1",
    "connect-timeout": "^1.8.0",
    "csv-stream": "^0.1.3",
    "datadog-metrics": "^0.4.0",
    "del": "^2.2.1",
    "dogapi": "^2.6.0",
    "ejs": "^2.5.6",
    "express": "^4.14.0",
    "hull-client": "1.1.4",
    "jsonwebtoken": "^7.4.3",
    "jwt-simple": "^0.5.0",
    "kue": "^0.11.5",
    "kue-ui": "^0.1.0",
    "lodash": "^4.13.1",
    "newrelic": "^1.39.0",
    "passport": "^0.3.2",
    "promise-streams": "^1.0.1",
    "raven": "^1.1.2",
    "raw-body": "^2.1.7",
    "request": "^2.72.0",
    "sns-validator": "^0.3.0",
    "sqs-consumer": "^3.6.1",
    "supply": "0.0.4",
    "urijs": "^1.18.7"
  },
  "peerDependencies": {
    "aws-sdk": "^2.81.0",
    "sqs-consumer": "^3.6.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-lodash": "^3.2.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.45.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-load-plugins": "^1.2.0",
    "gulp-mocha": "^4.3.1",
    "gulp-plumber": "^1.1.0",
    "isparta": "^4.0.0",
    "minihull": "0.0.7",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.0",
    "node-mocks-http": "^1.6.1",
    "nyc": "^11.0.3",
    "rimraf": "^2.6.0",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0",
    "updtr": "^1.0.0"
  },
  "nodeBoilerplateOptions": {
    "mochaGlobals": [
      "stub",
      "spy",
      "expect"
    ]
  },
  "nyc": {
    "include": [
      "src"
    ]
  }
}
