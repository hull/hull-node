    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "isparta": "^4.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "updtr": "^0.2.1"
