{
  "name": "hull",
  "version": "0.9.6",
  "description": "A Node.js client for hull.io",
  "main": "lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/hull/hull-node.git"
  },
  "homepage": "https://github.com/hull/hull-node",
