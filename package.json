    "jwt-simple": "^0.5.0",
    "lodash": "^4.13.1",
    "object-mapper": "^3.0.1",
    "passport": "^0.3.2",
    "raw-body": "^2.1.7",
    "request": "^2.72.0",
    "restler": "^3.4.0",
    "sns-validator": "^0.2.1",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
